{
  "parser": "babel-eslint",
  "plugins": ["react"],
  "extends": ["plugin:react/recommended"],
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "import/no-unresolved": 0,
    "dot-notation": "warn",
    "import/imports-first": "warn",
    "no-console": "warn",
    "no-duplicate-imports": "error",
    "sort-imports": "off",
    "no-unused-vars": "error"
  }
}
